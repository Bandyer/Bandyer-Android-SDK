<resources>

    <declare-styleable name="ConfigurationCategoryStyleable">
        <attr name="title" />
    </declare-styleable>

    <declare-styleable name="ConfigurationStyleable">
        <attr name="title" format="string" />
        <attr name="subtitle" format="string" />
        <attr name="summary" format="string" />
    </declare-styleable>

    <declare-styleable name="ConfigurationCheckboxStyleable">
        <attr name="title" />
        <attr name="subtitle" />
        <attr name="summary" />
        <attr name="checked" format="boolean" />
    </declare-styleable>

    <declare-styleable name="ConfigurationListStyleable">
        <attr name="title" />
        <attr name="subtitle" />
        <attr name="summary" />
        <attr name="entries" format="reference" />
        <attr name="entryValues" format="reference" />
        <attr name="defaultValue" format="string" />
    </declare-styleable>

    <declare-styleable name="ConfigurationEditTextStyleable">
        <attr name="title" />
        <attr name="subtitle" />
        <attr name="summary" />
        <attr name="hint" format="string" />
    </declare-styleable>

    <declare-styleable name="ConfigurationImageTextStyleable">
        <attr name="title" />
        <attr name="subtitle" />
        <attr name="summary" />
        <attr name="imageUri" format="string" />
        <attr name="imageName" format="string" />
    </declare-styleable>

</resources>